#include "main.h"
/**
* _strcmp - compare tow strings 
* @dest: destination string to compare
* @src: source string to compare
* Return: difference between both
*/
int _strcmp(char *s1, char *s2)
{
int i;
i = 0;
while (s1[i] != '\0' && s2[i] != '\0')
{
if (s1[i] != s2[i])
{
return (s1[i] - s2[i]);
}
i++;
}
return (0);
}
